{
	"name": "inpulse-whatsapp",
	"version": "0.0.1",
	"description": "Service that integrate inPulse with Whatsapp.",
	"main": "dist/main.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules --watch .env src/main.ts",
		"dev:frontend": "node scripts/build-frontend.js --watch",
		"start": "ts-node src/main.ts",
		"build:frontend": "node scripts/build-frontend.js",
		"build:frontend:watch": "node scripts/build-frontend.js --watch",
		"u": "npm i @in.pulse-crm/sdk@latest",
		"dev:instances": "cd ../instances-service && npm run dev",
		"dev:users": "cd ../users-service && npm run dev",
		"dev:customers": "cd ../customers-service && npm run dev",
		"dev:files": "cd ../files-service && npm run dev",
		"dev:socket": "cd ../socket-service && npm run dev",
		"redis:start": "bash scripts/redis.sh start",
		"redis:stop": "bash scripts/redis.sh stop",
		"redis:restart": "bash scripts/redis.sh restart",
		"redis:logs": "bash scripts/redis.sh logs",
		"redis:cli": "bash scripts/redis.sh cli",
		"redis:stats": "bash scripts/redis.sh stats",
		"redis:keys": "bash scripts/redis.sh keys",
		"redis:clear": "bash scripts/redis.sh clear",
		"redis:monitor": "bash scripts/redis.sh monitor",
		"redis:ui": "bash scripts/redis.sh ui"
	},
	"author": "Renan G. Dutra",
	"license": "ISC",
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@in.pulse-crm/sdk": "^2.13.1",
		"@in.pulse-crm/utils": "^1.3.0",
		"@mui/icons-material": "^7.3.4",
		"@mui/material": "^7.3.4",
		"@ngrok/ngrok": "^1.5.2",
		"@prisma/client": "^6.4.1",
		"@rgranatodutra/http-errors": "^1.0.7",
		"axios": "^1.8.4",
		"class-transformer": "^0.5.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-async-errors": "^3.1.1",
		"fluent-ffmpeg": "^2.1.3",
		"form-data": "^4.0.4",
		"html-pdf": "^3.0.1",
		"ioredis": "^5.8.1",
		"mime-types": "^3.0.1",
		"multer": "^1.4.5-lts.1",
		"node-cron": "^3.0.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"reactflow": "^11.11.4",
		"whatsapp-web.js": "github:BenyFilho/whatsapp-web.js#fix-getContact"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/fluent-ffmpeg": "^2.1.27",
		"@types/ioredis": "^4.28.10",
		"@types/mime-types": "^2.1.4",
		"@types/multer": "^1.4.12",
		"@types/node-cron": "^3.0.11",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"esbuild": "^0.25.11",
		"prettier": "3.5.2",
		"prisma": "^6.10.1",
		"ts-node-dev": "^2.0.0"
	}
}
